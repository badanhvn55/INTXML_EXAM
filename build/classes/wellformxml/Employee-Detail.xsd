<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xs:element name="Employee-Detail">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Employee"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Employee">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id"/>
                <xs:element name="name"/>
                <xs:element name="email"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
